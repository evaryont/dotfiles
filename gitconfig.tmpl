[github]
	user = evaryont
	token = $(cat ~/.github-token)
[user]
	name = Colin Shea
	email = evaryont@saphrix.com
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
	excludesfile = $(realpath ~/.gitignore)
[branch]
	autosetupmerge = true
[alias]
	a = add
	br = branch
	st = status
	com = commit
	unpushed = log HEAD..origin
[web]
	browser = open
[browser "open"]
	cmd = $(which open-browser)

